{
  "name": "shark-cleaner",
  "version": "1.1.4",
  "description": "A cleaning tool to help you quickly clear cache garbage.",
  "main": "main.js",
  "scripts": {
    "electron": "cross-env NODE_ENV=development RUN_TYPE=electron ELECTRON_ENABLE_LOGGING=1 node ./scripts/server.js",
    "web": "cross-env NODE_ENV=development RUN_TYPE=web node ./scripts/server.js",
    "cli": "NODE_ENV=local node cli/index.js",
    "test": "mocha",
    "build-ts": "tsc",
    "build-mac": "electron-builder --mac",
    "update": "auto-vers -t",
    "compile-web": "RUN_TYPE=web webpack --config ./scripts/webpack-prod.js --color --progress",
    "compile-electron": "RUN_TYPE=electron webpack --config ./scripts/webpack-prod.js --color --progress"
  },
  "build-config": {
    "appid": "001",
    "version": "001",
    "env": "production"
  },
  "keywords": [
    "cleaner",
    "node_modules",
    "clean"
  ],
  "bin": {
    "shark": "./cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hua1995116/shark-cleaner.git"
  },
  "author": "hua1995116 <qiufneghyf@gmail.com> (https://github.com/hua1995116)",
  "bugs": {
    "url": "https://github.com/hua1995116/shark-cleaner/issues"
  },
  "homepage": "https://github.com/hua1995116/shark-cleaner#readme",
  "license": "MIT",
  "dependencies": {
    "home": "^2.0.0",
    "load-json-file": "^6.2.0",
    "multimatch": "^4.0.0",
    "nodejs-fs-utils": "^1.2.4",
    "npm-cache-path": "^2.0.0",
    "rimraf": "^3.0.2",
    "username": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-async-generators": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.9.4",
    "@types/koa": "^2.11.2",
    "@types/node": "^13.9.0",
    "@types/socket.io": "^2.1.4",
    "antd": "^4.0.2",
    "auto-vers": "^1.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "colors": "^1.4.0",
    "commander": "^5.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.4",
    "i18next": "^19.3.2",
    "i18next-browser-languagedetector": "^4.0.2",
    "koa": "^2.11.0",
    "koa-send": "^5.0.0",
    "less": "^2.3.1",
    "less-loader": "3.x",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^4.17.15",
    "log-symbols": "^3.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.1",
    "normalize.css": "^8.0.1",
    "open": "^7.0.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-i18next": "^11.3.3",
    "react-router-dom": "^5.1.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "style-loader": "^0.19.0",
    "timeago.js": "^4.0.2",
    "typescript": "^3.8.3",
    "url-loader": "^0.6.2",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.20.0"
  },
  "engines": {
    "node": ">=10.10.0"
  },
  "packages": [
    "packages/*"
  ],
  "build": {
    "productName": "shark-cleaner",
    "appId": "com.electron.sharkcleaner",
    "copyright": "Copyright © year 蓝色的秋风",
    "asar": false,
    "files": [
      "package.json",
      "main.js",
      "dist/",
      "cli/",
      "main/",
      "render_process/",
      "node_modules/",
      "icon.ico"
    ],
    "npmRebuild": false,
    "dmg": {
      "artifactName": "shark-cleaner.dmg",
      "contents": [
        {
          "type": "link",
          "path": "/Applications",
          "x": 410,
          "y": 150
        },
        {
          "type": "file",
          "x": 130,
          "y": 150
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "shortcutName": "shark-cleaner"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "icon.ico"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "directories": {
      "buildResources": "resources",
      "output": "electron/"
    }
  }
}
